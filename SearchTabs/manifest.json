{
    "description": "Search any word across all your open tabs",
    "manifest_version": 2,
    "name": "Search Tabs",
    "permissions": [
        "find",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "32": "icons/logo_32.png",
        "64": "icons/logo_64.png",
        "128": "icons/logo_128.png"
    },
    "browser_action": {
        "browser_style": true,
        "default_title": "Search Tabs",
        "default_icon": {
            "64": "icons/logo_64.png"
        }
    },
    "version": "1.0",
    "applications": {
        "gecko": {
            "strict_min_version": "57.0a1"
        }
    }
}